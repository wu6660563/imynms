<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
         xmlns:s="library://ns.adobe.com/flex/spark" 
         xmlns:mx="library://ns.adobe.com/flex/mx" width="100%" height="100%">
    <s:layout>
        <s:BasicLayout/>
    </s:layout>
    <fx:Declarations>
        <!-- 将非可视元素（例如服务、值对象）放在此处 -->
    </fx:Declarations>

    <s:DataGrid id="userList" width="100%" height="100%" >
        <s:columns>
            <s:ArrayList>
                <s:GridColumn dataField="userName" headerText="用户名"/>
                <s:GridColumn dataField="phoneNumber" headerText="电话号码"/>
                <s:GridColumn dataField="mobileNumber" headerText="手机号码"/>
                <s:GridColumn dataField="email" headerText="电子邮箱"/>
            </s:ArrayList>
        </s:columns>
    </s:DataGrid>
</s:Group>
